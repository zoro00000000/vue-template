{
    "name": "liong-cli",
    "version": "1.0.0",
    "description": "A mobile tempalte of vue2",
    "main": "index.js",
    "scripts": {
        "dev": "sudo cross-env NODE_ENV=development VUE_APP_BUILD_ENV=dev node ./build/i.dev.js",
        "dev:test": "sudo cross-env NODE_ENV=development VUE_APP_BUILD_ENV=test node ./build/i.dev.js",
        "dev:staging": "sudo cross-env NODE_ENV=development VUE_APP_BUILD_ENV=staging node ./build/i.dev.js",
        "dev:prod": "sudo cross-env NODE_ENV=development VUE_APP_BUILD_ENV=prod node ./build/i.dev.js",
        "start": "node ./build/dev-server.js",
        "build": "cross-env NODE_ENV=production VUE_APP_BUILD_ENV=prod node ./build/i.build.js",
        "build:test": "cross-env NODE_ENV=production VUE_APP_BUILD_ENV=dev node ./build/i.build.js",
        "build:staging": "cross-env NODE_ENV=production VUE_APP_BUILD_ENV=staging node ./build/i.build.js",
        "test": "cross-env NODE_ENV=development VUE_APP_BUILD_ENV=test webpack-dev-server --open --config=./build/webpack.dev.conf.js",
        "build-pro": "webpack --config=./build/webpack.prod.conf.js",
        "upload": "node ./build/upload-files.js",
        "new:project": "node ./build/create-project.js",
        "upload:prod": "node ./build/merge-project.js"
    },
